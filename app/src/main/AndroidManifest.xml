<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.widadwitiayasa.mytusk">

    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:supportsRtl="true"
        android:theme="@style/MyMaterialTheme">
        <activity android:name=".activity.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.FAB"
            android:label="@string/title_activity_fab"
            android:theme="@style/MyMaterialTheme" />
        <activity
            android:name=".activity.MainActivity"
            android:label="@string/title_activity_drawer"
            android:theme="@style/MyMaterialTheme" />
        <activity android:name=".activity.Drawer" />
        <activity android:name=".activity.Register"></activity>
        <activity android:name=".activity.Pop"></activity>
    </application>

</manifest>